<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        @helper InlineHelper(string[] str)
            {
                <ul>
                    @foreach(var item in str)
                        {
                            <li>
                                @item
                            </li>
                        }
                </ul>
    }
    </div>
    <div>
        <label>Examle 1 - Name List </label>
        <div>
            @InlineHelper(new string[] {"Vidur","Agarwal","Dot","Net"})
        </div>
    </div>
</body>
</html>
